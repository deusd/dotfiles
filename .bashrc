# dir navigation
alias ".."="cd .."
alias "..."="cd ..; cd .."

# git
alias gb="git branch"
alias gba="git branch -a"
alias gbr="git branch -r"
alias gco="git checkout"
alias gcob="git checkout -b"
alias gd="git diff"
alias gds="git diff --staged"
alias gf="git fetch"
alias gfa="git fetch --all"
alias gi="git commit"
alias gia="git commit --amend"
alias gp="git pull"
alias gs="git status"

# ls
alias la="ls -la"

